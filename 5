public class SumOfNegativeNumbers {
    public static void main(String[] args) {
        int[] array = {10, -21, 4, -45, 66, -93, 22, -8};
 int sumOfNegatives = 0;
 for (int element : array) {
            if (element < 0) {  // Check if the element is negative
                sumOfNegatives += element;
            }
        }

          System.out.println("Sum of negative numbers in the array: " + sumOfNegatives);
    }
}

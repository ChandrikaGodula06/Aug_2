public class PositiveNumbers {
    public static void main(String[] args) {
        int[] array = {10, -21, 4, -45, 66, -93, 22, -8};
        System.out.println("Positive numbers in the array:");
        for (int element : array) {
            if (element > 0) { 
                System.out.println(element);
            }
        }
    }
}

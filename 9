public class SumOfArrayElements {
    public static void main(String[] args) 
        int[] array = {10, 21, 4, 45, 66, 93, 22};
        int sum = 0;
        for (int element : array) {
            sum += element;
        }
        System.out.println("The sum of all elements in the array is: " + sum);
    }
}

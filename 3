package arrays;
public class odd {
	public static void main(String args[]) {
		int a[]= {20,30,10,2,1,40,11,7,13,21};
	  int count_odd=0;
	  for(int i=0;i<=a.length-1;i++) {
			if(a[i]%2==0)
			else {
				count_odd++;
			}
			int count=0;
			for(int j=1;j<=a[i];j++) {
				if(a[i]%j==0) {
					count++;
				}
			}
			System.out.println(count_odd+" count of odd elements");
			
		}
}
		
		
